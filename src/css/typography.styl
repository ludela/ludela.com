
@font-face {
  font-family 'Freeland'
  src url("/fonts/Freeland.otf") format("opentype")
}

@font-face {
  font-family: 'ludela';
  src:  url('/fonts/ludela.eot?erta0y');
  src:  url('/fonts/ludela.eot?erta0y#iefix') format('embedded-opentype'),
    url('/fonts/ludela.ttf?erta0y') format('truetype'),
    url('/fonts/ludela.woff?erta0y') format('woff'),
    url('/fonts/ludela.svg?erta0y#ludela') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon icon-"], [class*=" icon icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'ludela' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

